{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
        // "plugin:next/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
           
    "rules": {
        "react/prop-types": "off",
        "react/react-in-jsx-scope": "off"
        // "@next/next/no-page-custom-font": "off"
    }
      
}
